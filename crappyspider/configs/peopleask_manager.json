{
    "start_urls": ["http://peopleask.local/manager"],
    "allowed_domains": ["peopleask.local"],
    "patterns": [
        "/manager/\\?",
        "/manager/article/\\?",
        "/manager/article/[0-9]+",
        "/manager/request/[0-9]+",
        "/manager/requests/\\?",
        "/manager/auto-answer/\\?",
        "/manager/requests/mine/\\?",
        "/manager/employee/.+",
        "/manager/category/.+",
        "/administration/form/\\?",
        "/administration/form/[0-9]+",
        "/administration/manager/[0-9]+",
        "/administration/role/[0-9]+",
        "/administration/category/.+"
    ],
    "excluded_patterns": [
        "/sudo/",
        "/unsudo/",
        "/logout/"
    ],
    "credential": {
        "email": "admin",
        "password": "admin1234"
    },
    "login_error_selector": ".errorlist"
}
